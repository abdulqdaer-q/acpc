{
  "kind": "collectionType",
  "collectionName": "team_members",
  "info": {
    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "Team Member",
    "description": "Team member information"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "name": {
      "type": "string",
      "required": false
    },
    "email": {
      "type": "email",
      "required": false
    },
    "student_id": {
      "type": "string",
      "required": false
    },
    "major": {
      "type": "string",
      "required": false
    },
    "year": {
      "type": "integer",
      "required": false,
      "min": 1,
      "max": 6
    },
    "full_name_arabic": {
      "type": "string",
      "required": false
    },
    "birth_date": {
      "type": "date",
      "required": false
    },
    "tshirt_size": {
      "type": "enumeration",
      "enum": ["S", "M", "L", "XL", "XXL", "XXXL"],
      "required": false
    },
    "faculty": {
      "type": "enumeration",
      "enum": ["ITE", "CS", "Other"],
      "required": false
    },
    "role": {
      "type": "enumeration",
      "enum": ["member", "captain"],
      "default": "member"
    },
    "team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "members"
    }
  }
}
