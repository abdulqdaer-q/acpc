{
  "kind": "collectionType",
  "collectionName": "team_members",
  "info": {
    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "Team Member",
    "description": "Team member information"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "student_id": {
      "type": "string",
      "required": true
    },
    "major": {
      "type": "string",
      "required": true
    },
    "year": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 5
    },
    "role": {
      "type": "enumeration",
      "enum": ["member", "captain"],
      "default": "member"
    },
    "team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "members"
    }
  }
}
